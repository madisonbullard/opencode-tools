{
	"$schema": "https://json.schemastore.org/package",
	"name": "@madisonbullard/opencode-scripts",
	"version": "0.2.0",
	"description": "Scripts for opencode-tools",
	"author": "madisonbullard",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/madisonbullard/opencode-tools.git",
		"directory": "packages/scripts"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "bun run build",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"opencode",
		"scripts",
		"utilities"
	],
	"devDependencies": {
		"@types/bun": "catalog:",
		"typescript": "catalog:"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./ingest-session": {
			"types": "./dist/ingest-session.d.ts",
			"default": "./dist/ingest-session.js"
		},
		"./detect-repo": {
			"types": "./dist/detect-repo.d.ts",
			"default": "./dist/detect-repo.js"
		},
		"./path-utils": {
			"types": "./dist/path-utils.d.ts",
			"default": "./dist/path-utils.js"
		},
		"./session-archive": {
			"types": "./dist/session-archive.d.ts",
			"default": "./dist/session-archive.js"
		}
	}
}
